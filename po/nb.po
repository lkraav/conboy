#: ../src/interface.c:666
msgid "Delete note"
msgstr "Slett notat"

#. Translators: Monospaced (non-proportional) font.
#: ../src/interface.c:668 ../src/interface.c:804 ../src/interface.c:845
msgid "Fixed width"
msgstr "Fast bredde"

#. Translators: Emphasise text with a background colour.
#: ../src/interface.c:670 ../src/interface.c:802 ../src/interface.c:843
msgid "Highlight"
