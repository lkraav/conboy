#: ../src/interface.c:673
msgid "Increase indent"
msgstr "Øk innrykk"

#. Translators: Add a [hyper]link to another note.
#: ../src/interface.c:675
msgid "Link"
msgstr "Lenke"

#. Translators: Create a new note.
#: ../src/interface.c:677
msgid "New note"
msgstr "Nytt notat"

#. Translators: Open an existing note.
#: ../src/interface.c:679
msgid "Open note"
msgstr "Åpne notat"

