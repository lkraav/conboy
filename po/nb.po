#: ../src/interface.c:681 ../src/settings_window.c:443
#: ../src/settings_window.c:451
msgid "Settings"
msgstr "Innstillinger"

#. Translators: Quit the program.
#: ../src/interface.c:683
msgid "Quit"
msgstr "Avslutt"

#. Translators: Italic (cursive) font.
#: ../src/interface.c:685 ../src/interface.c:800 ../src/interface.c:841
msgid "Italic"
msgstr "Kursiv"

#. Translators: Crossed out text.
#: ../src/interface.c:687 ../src/interface.c:801 ../src/interface.c:842
